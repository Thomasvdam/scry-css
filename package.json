{
  "name": "howaboutless",
  "version": "0.0.1",
  "description": "Helper lib for auto-discovery of LESS variables and mixins",
  "main": "index.js",
  "scripts": {
    "test": "mocha test",
    "test:watch": "npm run test -- -w",
    "test:report": "mocha test --recursive --reporter mochawesome --reporter-options reportDir=mocha-report,reportName=test-report,reportTitle=TestReport",
    "test:coverage": "istanbul cover node_modules/.bin/_mocha -- -- test",
    "lint": "eslint src/**/*.js"
  },
  "author": "Ovidiu Bute <ovidiu.bute@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "lodash": "4.14.1",
    "node-dir": "0.1.15",
    "promised-io": "0.3.5"
  },
  "keywords": [
    "less",
    "helper",
    "parser",
    "mixin"
  ],
  "devDependencies": {
    "chai": "3.5.0",
    "eslint": "3.2.0",
    "eslint-config-airbnb-base": "5.0.1",
    "eslint-plugin-import": "1.12.0",
    "istanbul": "0.4.4",
    "mocha": "2.5.3",
    "mochawesome": "1.4.0"
  },
  "engines" : {
    "npm" : "~6.0.0"
  }
}
